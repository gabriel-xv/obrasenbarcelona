<div class="container">

<h1><%= @article.title %></h1>
<div class="article-image">
  <%= cl_image_tag @article.photos.first.key %>
</div>
<%= @article.description %>

<h3>Proyecto de reforma</h3>

<h5>Photos</h5>
<ul>
  <% @article.photos.each do |photo| %>
    <%= cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
  <% end %>
</ul>


</div>

